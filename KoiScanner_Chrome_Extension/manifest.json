{
"manifest_version": 2,
"name": "KoiScanner",
"description": "This extension shows a Google Image search result for the current page",
"version": "1.0",
"browser_action": 
    {
    "default_icon": "/images/icon-16x16.png",
    "default_popup": "/html/popup.html",
    "default_title": "My custom google page!"
    },
    "permissions": [
        "tabs"
    ]
}